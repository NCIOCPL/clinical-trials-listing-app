		"url": "https://github.com/nciocpl/clinical-trials-listing-app/issues"
	},
	"homepage": ".",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"dependencies": {
		"axios": "^1.6.7",
		"camelcase": "^5.3.1",
		"core-js": "^3.6.4",
		"dom-scroll-into-view": "^2.0.1",
		"dotenv": "8.2.0",
		"dotenv-expand": "5.1.0",
		"es6-promise": "^4.2.8",
		"fs-extra": "^8.1.0",
		"history": "^5.0.0",
		"prop-types": "^15.7.2",
		"qs": "^6.10.1",
		"react": "^16.13.1",
		"react-app-polyfill": "^1.0.6",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.0.0-beta.2",
		"react-router": "^6.0.0-alpha.1",
		"react-router-dom": "^6.0.0-alpha.1",
		"react-tracking": "^7.3.0",
		"resolve": "1.15.0"
	},
	"engines": {
		"node": ">=20.9.0 <21.0.0",
		"npm": ">=10.0.0 <12.0.0"
	},
	"scripts": {
		"start": "node scripts/start.js",
		"start:silent": "BROWSER=none npm start",
		"debug": "node --inspect --inspect-brk ./node_modeles/webpack/bin/webpack.js",
		"dev:debug": "npm run debug -- --mode development",
		"build:cgdp-legacy": "node scripts/generate-cgdp-legacy.js",
		"build:cra": "node scripts/build.js",
		"build": "npm run build:cra && npm run build:cgdp-legacy",
		"test": " npm run pa11y-tests && npm run unit-tests && npm run cy:ci && npm run merge-coverage",
		"test:it": "NODE_ENV=test cypress open",
		"test:pa11y": "npm run pa11y-ci",
		"pa11y-tests": "start-server-and-test start http://localhost:3000 pa11y",
		"pa11y": "pa11y-ci",
		"cy:ci": "start-server-and-test start:silent http://localhost:3000 integration-tests",
		"unit-tests": "jest",
		"integration-tests": "NODE_ENV=test cypress run && nyc report",
		"merge-coverage": "npm run mc:pre && npm run mc:merge && npm run mc:report",
		"mc:pre": "node scripts/pre-merge-coverage",
		"mc:merge": "nyc merge coverage/tmp coverage/merged/coverage-final.json",
		"mc:report": "nyc report --nycrc-path=merged.nyc.config.js",
		"format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
		"lint": "npm run lint:src && npm run lint:cypress",
		"lint:src": "eslint src --ext .js,.jsx",
		"lint:cypress": "eslint cypress/e2e --ext .js,.jsx",
		"lint:fix": "npm run lint:src:fix && npm run lint:cypress:fix",
		"lint:src:fix": "eslint src --ext .js,.jsx --fix",
		"lint:cypress:fix": "eslint cypress/e2e --ext .js,.jsx --fix"
	},
	"browserslist": {
		"production": [
			">1%",
			"last 2 versions",
			"not dead",
			"not IE 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version",
			"ie 11"
		]
	},
	"cypress-cucumber-preprocessor": {
		"nonGlobalStepDefinitions": true,
		"stepDefinitions": "cypress/e2e",
		"commonPath": "cypress/e2e/common"
	},
	"devDependencies": {
		"@babel/core": "7.9.0",
		"@babel/eslint-parser": "^7.23.10",
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"@cypress/code-coverage": "^3.12.45",
		"@nciocpl/eslint-config-react": "^1.0.0",
		"@svgr/webpack": "^8.1.0",
		"@testing-library/jest-dom": "^5.11.9",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^12.8.3",
		"@types/express": "^4.17.2",
		"babel-jest": "^29.3.1",
		"babel-loader": "^8.2.5",
		"babel-plugin-istanbul": "^7.0.0",
		"babel-plugin-named-asset-import": "^0.3.6",
		"babel-preset-react-app": "^9.1.2",
		"buffer": "^6.0.3",
		"case-sensitive-paths-webpack-plugin": "2.3.0",
		"css-loader": "^6.10.0",
		"css-minimizer-webpack-plugin": "^7.0.0",
		"cypress": "^13.6.6",
		"cypress-cucumber-preprocessor": "^4.3.1",
		"deep-equal": "^2.1.0",
		"eslint": "^8.0.1",
		"eslint-webpack-plugin": "^4.0.1",
		"eslint-plugin-jest": "^28.7.0",
		"eslint-plugin-jest-dom": "^5.4.0",
		"eslint-plugin-testing-library": "^6.2.2",
		"html-webpack-plugin": "^5.6.0",
		"https-browserify": "^1.0.0",
		"identity-obj-proxy": "3.0.0",
		"istanbul-lib-coverage": "^3.0.0",
		"jest": "^29.2.1",
		"jest-environment-jsdom": "^29.3.1",
		"jest-environment-jsdom-global": "^4.0.0",
		"jest-resolve": "^29.3.1",
		"jest-watch-typeahead": "^2.1.1",
		"mini-css-extract-plugin": "^1.6.2",
		"nock": "^11.7.2",
		"nyc": "^15.1.0",
		"optimize-css-assets-webpack-plugin": "^6.0.1",
		"pa11y-ci": "^3.0.1",
		"pnp-webpack-plugin": "1.6.4",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-loader": "^8.1.1",
		"postcss-normalize": "^10.0.1",
		"postcss-prefix-selector": "^1.16.0",
		"postcss-preset-env": "^9.5.2",
		"postcss-safe-parser": "^7.0.0",
		"process": "^0.11.10",
		"react-dev-utils": "^12.0.1",
		"resolve-url-loader": "^5.0.0",
		"sass": "^1.63.6",
		"sass-loader": "^10.3.1",
		"semver": "^6.3.1",
		"start-server-and-test": "2.0.0",
		"stream-http": "^3.2.0",
		"style-loader": "^2.0.0",
		"terser-webpack-plugin": "^4.1.0",
		"ts-pnp": "1.1.6",
		"url": "^0.11.3",
		"webpack": "^5.90.3",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.11.1",
		"webpack-manifest-plugin": "3.0.0",
		"workbox-webpack-plugin": "4.3.1"
	}
}
